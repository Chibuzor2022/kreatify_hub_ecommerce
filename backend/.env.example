# Server
PORT=5000

# MongoDB connection string (DO NOT use your real password/token here)
MONGO_URI=mongodb+srv://your_user:your_password@your_cluster.mongodb.net/your_db_name

# JWT secret key for signing tokens
JWT_SECRET=your_jwt_secret

# Paystack secret key
PAYSTACK_SECRET_KEY=your_paystack_secret

# Paystack public key (optional here â€” frontend usually uses it)
PAYSTACK_PUBLIC_KEY=your_paystack_public

# Cloudinary credentials (if using for image uploads)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# CORS Origin (if needed)
CLIENT_URL=http://localhost:5173
